{
  "name": "mollitianon",
  "version": "1.0.0",
  "description": "Functional programming standard library for TypeScript ",
  "repository": {
    "type": "git",
    "url": "https://github.com/ElbertTremblay/mollitianon.git"
  },
  "author": "ModestoStehr",
  "license": "ISC",
  "sideEffects": false,
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "src/.+\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true
  },
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "module": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "import": {
        "types": "./es/index.d.ts",
        "default": "./es/index.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
    "./*": {
      "module": {
        "types": "./esm/*.d.ts",
        "default": "./esm/*.js"
      },
      "import": {
        "types": "./es/*.d.ts",
        "default": "./es/*.js"
      },
      "require": {
        "types": "./*.d.ts",
        "default": "./*.js"
      }
    }
  },
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@zoltu/typescript-transformer-append-js-extension": "1.0.1",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "coveralls": "3.1.1",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "ttypescript": "1.5.15",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@types/json-schema": "7.0.11"
  },
  "bugs": {
    "url": "https://github.com/ElbertTremblay/mollitianon.git/issues"
  },
  "homepage": "https://github.com/ElbertTremblay/mollitianon.git"
}